<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" >

    <!--描边：创建一个矩形-->
    <stroke android:width="1dp"
        android:color="#fff"/>
    <!--将矩形四个角添加圆边-->
    <corners android:radius="200dp"/>
    <!-- 填充的颜色 -->
    <solid android:color="@android:color/transparent"/>
    <!-- 各方向的间隔 -->
    <padding
        android:top="5dp"
        android:bottom="5dp"
        android:right="5dp"
        android:left="5dp"/>

</shape>
